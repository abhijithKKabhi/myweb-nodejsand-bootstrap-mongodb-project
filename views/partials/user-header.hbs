<header class="">
  <div class="row">
    <div class="col-md-6 col-9">
      <h3 class="p-2 fixed-top" style="color: white; font-weight: 600;">MY <span style="color: blue;">WEB</span></h3>

    </div>
    <div class="col-md-6 col-3">
      <nav class="navbar navbar-expand-lg navbar-dark shadow-5-strong fixed-top">
        <div class="container-fluid">

          <button class="navbar-toggler ms-auto" type="button" data-bs-toggle="collapse"
            data-bs-target="#navbarSupportedContent" aria-controls="navbarSupportedContent" aria-expanded="false"
            aria-label="Toggle navigation">
            <span class="navbar-toggler-icon"></span>
          </button>
          <div class="collapse navbar-collapse" id="navbarSupportedContent">
            <ul class="navbar-nav ms-auto mb-2 mb-lg-0 ">
              <li class="nav-item ">
                <a class="nav-link active" aria-current="page" href="/user/user-home">Home</a>
                </li>
                 <li class="nav-item">
                 <a class="nav-link active" aria-current="page" href="/user/show-apps">My&nbsp;Apps</a>
                </li>
                <li class="nav-item">
                 <a class="nav-link active" aria-current="page" href="/user/show-clicks">My&nbsp;Clicks </a>
                </li>
                <li class="nav-item">
                  {{#if user}}
                 <p class="nav-link active">{{datas.firstName}}.{{datas.lastName}}</p>
                 {{/if}}
                </li>
                
               </ul>
               <div class="d-grid gap-2 d-md-flex justify-content-md-end">
              {{#if user}}
              
              <a href="/logout" class="btn btn-light">LogOut</a>
              {{else}}
              <a href="/user/create-account" class="btn btn-success">SignUp</a>
              <a href="/" class="btn btn-light">LogIn</a>
              {{/if}}
            </div>

          </div>
        </div>
      </nav>
    </div>
  </div>
</header>